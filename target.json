{
  "name": "wrd-frdm-k64f-gcc",
  "version": "1.0.0",
  "description": "Freescale FRDM-K64F target with Wearable Reference Design configurations.",
  "licenses": [
    {
      "url": "https://spdx.org/licenses/Apache-2.0",
      "type": "Apache-2.0"
    }
  ],
  "inherits": {
    "frdm-k64f-gcc": "*"
  },
  "keywords": [
    "mbed-target:wrd-frdm-k64f",
    "k64f",
    "watch",
    "gcc"
  ],
  "similarTo": [
    "wrd-ui",
    "framebuffer-1bit",
    "matrixlcd-printf-1bit",
    "wrd-rgb-led",
    "rgb-led-pwm",
    "wrd-message-center",
    "message-center-spi-master"
  ],
  "config": {
    "mbed-os": {
      "stdio": {
        "default-baud": 115200
      }
    },
    "hardware": {
      "wearable-reference-design": {
        "battery": {
          "present": false
        },
        "battery-backup": {
          "present": false
        },
        "battery-gauge": {
          "present": false
        },
        "bit-band": {
          "present": false
        },
        "bluetooth-le": {
          "present": false
        },
        "button": {
          "present": true,
          "forward": {
            "gpio": {
              "pin": "PTA4"
            }
          },
          "backward": {
            "gpio": {
              "pin": "PTC6"
            }
          }
        },
        "buzzer": {
          "present": false
        },
        "external-gpio": {
          "present": false
        },
        "external-sram": {
          "present": false
        },
        "gps": {
          "present": false
        },
        "interface": {
          "i2c": {
            "size": 1,
            "i2c0": {
              "name": "i2c0",
              "sda": "PTE25",
              "scl": "PTE24"
            }
          },
          "spi": {
            "size": 0
          }
        },
        "message-center": {
          "present": true,
          "transport": {
            "size": 0
          }
        },
        "micro-sd": {
          "present": false
        },
        "nfc": {
          "present": false
        },
        "output-compare": {
          "present": false
        },
        "rfid": {
          "present": false
        },
        "rgb-led": {
          "present": true,
          "red": {
            "pwm": {
              "pin": "PTB22"
            }
          },
          "green": {
            "pwm": {
              "pin": "PTE26"
            }
          },
          "blue": {
            "pwm": {
              "pin": "PTB21"
            }
          }
        },
        "screen": {
          "present": false
        },
        "sensor-accelerometer": {
          "present": false
        },
        "sensor-gyroscope": {
          "present": false
        },
        "sensor-magnetometer": {
          "present": false
        },
        "sensor-battery-current": {
          "present": false
        },
        "sensor-battery-charging-current": {
          "present": false
        },
        "sensor-battery-voltage": {
          "present": false
        },
        "sensor-fingerprint": {
          "present": false
        },
        "sensor-temperature": {
          "present": false
        },
        "swo": {
          "present": false
        },
        "touch": {
          "present": false
        },
        "vibration-motor": {
          "present": false
        },
        "watchdog": {
          "present": false
        }
      }
    }
  }
}
